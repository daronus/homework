# Create the subjects and grades lists
subjects = ["physics", "calculus", "poetry", "history"]
grades = [98, 97, 85, 88]

# Manually create the gradebook by combining subjects and grades
gradebook = []
for i in range(len(subjects)):
    gradebook.append([subjects[i], grades[i]])

# Print the initial gradebook
print(gradebook)

# Add "computer science" and 100 to the gradebook
gradebook.append(["computer science", 100])

# Add "visual arts" and 93 to the gradebook
gradebook.append(["visual arts", 93])

# Modify the grade for "visual arts" to add 5 points
for item in gradebook:
    if item[0] == "visual arts":
        item[1] += 5

# Find and remove the grade for "poetry," then add "Pass" to the gradebook
for item in gradebook:
    if item[0] == "poetry":
        gradebook.remove(item)
        gradebook.append(["poetry", "Pass"])

# Define last semester's gradebook
last_semester_gradebook = [["politics", 80], ["latin", 96], ["dance", 97], ["architecture", 65]]

# Combine last semester's gradebook and the current gradebook
full_gradebook = last_semester_gradebook + gradebook

# Print the full gradebook
print(full_gradebook)
